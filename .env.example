# Discord Bot Token (required)
# Get this from the Discord Developer Portal
TOKEN=your_discord_bot_token_here

# Openai API Key (required for the /ask command)
# Get this from Open AI website
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=your_openai_model_name_here

# Server ID (required for server-specific commands)
# This is the Discord server ID where the bot will operate
SERVER_ID=your_discord_server_id_here

